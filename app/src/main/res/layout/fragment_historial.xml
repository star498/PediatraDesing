                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5sdp"
                    android:layout_marginEnd="@dimen/_5sdp"
                    android:text="Lucia Perez"
                    android:maxLines="1"
                    android:fontFamily="sans-serif"
                    android:ellipsize="end"
